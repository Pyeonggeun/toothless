<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mkfactory.toothless.donot.touch.mapper.ExternalSqlMapper">

	<select id="selectExternalByIdAndPassword" resultType="com.mkfactory.toothless.donot.touch.dto.ExternalInfoDto">
		SELECT * FROM tl_jic_external_info
		WHERE external_id = #{external_id}
		AND password = #{password}
	</select>


	
</mapper>